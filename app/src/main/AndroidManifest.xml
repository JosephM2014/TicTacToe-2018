<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="apps.yonga.com.tictactoe2">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".TwoPlayers"
            android:label="2 players"
            android:parentActivityName=".TwoPlayers" />
        <activity android:name=".Welcome">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".About"
            android:label="About"
            android:parentActivityName=".TwoPlayers" />
        <activity
            android:name=".SinglePlayer"
            android:label="Single Player"
            android:parentActivityName=".TwoPlayers" />
        <activity android:name=".Ultimate"
            android:parentActivityName=".Welcome"></activity>
    </application>

</manifest>